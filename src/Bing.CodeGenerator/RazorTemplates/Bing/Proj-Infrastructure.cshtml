@model BuildContext
@{ 
    var project = Model.Project;
}
<Project Sdk="Microsoft.NET.Sdk">
    <Import Project="..\..\common.props" />
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>@($"{project.Module}.Infrastructure")</AssemblyName>
        <RootNamespace>@($"{project.Module}.Infrastructure")</RootNamespace>
    </PropertyGroup>
    
    <ItemGroup>
        <PackageReference Include="Bing.AspNetCore" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.AspNetCore.Mvc" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.AutoMapper" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Datas.Dapper" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Ddd.Application" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Permissions" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Datas.EntityFramework.MySql" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Events" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Logs.Exceptionless" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Logs.NLog" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Caching.CSRedis" Version="$(BingFrameworkVersion)" />
        <PackageReference Include="Bing.Locks.CSRedis" Version="$(BingFrameworkVersion)" />
    </ItemGroup>

    <ItemGroup>
        <PackageReference Include="Bing.Biz" Version="1.2.1" />
        <PackageReference Include="Bing.Extensions.Swashbuckle" Version="1.2.2" />
        <PackageReference Include="Bing.Utils.Http" Version="1.2.3" />
        <PackageReference Include="CSRedisCore" Version="3.6.6" />
        <PackageReference Include="Swashbuckle.AspNetCore.Newtonsoft" Version="5.6.3" />
        <PackageReference Include="System.Diagnostics.DiagnosticSource" Version="4.7.1" />
        <PackageReference Include="IdentityModel" Version="3.10.10" />
        <PackageReference Include="DotNetCore.CAP" Version="3.1.2" />
        <PackageReference Include="DotNetCore.CAP.RabbitMQ" Version="3.1.2" />
    </ItemGroup>
</Project>