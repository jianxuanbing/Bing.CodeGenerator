@model BuildContext
@{ 
    var project = Model.Project;
}
<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <TargetFramework>netstandard2.1</TargetFramework>
        <AssemblyName>@(project.Module).Service</AssemblyName>
        <RootNamespace>@(project.Module).Service</RootNamespace>
    </PropertyGroup>

    <ItemGroup>
        <ProjectReference Include="..\@(project.Module).Data\@(project.Module).Data.csproj" />
    </ItemGroup>
</Project>